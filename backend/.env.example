# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key_here

# Application Configuration
TZ=Asia/Karachi
PORT=5000

# Gmail API Configuration (Optional - for local development)
# These are not needed in production as tokens are stored in Supabase
# GMAIL_CLIENT_ID=your_google_client_id
# GMAIL_CLIENT_SECRET=your_google_client_secret

# Scraping Configuration (Optional - defaults provided)
# GMAIL_QUERY_BASE=subject:("Class Schedule" OR schedule) in:inbox
# CHECK_HOUR_LOCAL=20
# CHECK_MINUTE_LOCAL=0
# NEXT_DAY_AVAILABLE_HOUR=17
# NEWER_THAN_DAYS=2
# ALLOWED_SEMESTERS=BS (SE) - 5C, BS (CS) - 7A
# DEBUG_PARSING=false